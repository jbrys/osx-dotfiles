[color]
  ui = auto
	ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = red
[alias]
	lg = log --pretty=format:'%C(auto)%h%d %C(white bold ul)%an%Creset:%C(green)%s %C(cyan bold)%ar' --graph
